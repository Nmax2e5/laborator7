#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main()
{
    FILE* in  = fopen("input.txt",  "r");
    char *c=(char*) malloc(1001 * sizeof(char));
    int len=0;
    char *ans=(char*) malloc(sizeof(char));

    while(fscanf(in, "%s", c)==1)
    {
        ans=(char*) realloc(ans, (len+strlen(c))*sizeof(char));

        for(int i=0;i<strlen(c);i++)
            ans[len++]=c[i];
    }

    printf("%s", ans);
}
